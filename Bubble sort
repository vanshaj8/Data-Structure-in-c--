#include<iostream>
using namespace std;
int arr[100];
void binary(int n)
{
	for(int i=0;i<n-1;i++)
	{
		for(int j=0;j<n-1;j++)
		{
			if(arr[j]>arr[j+1])
			{
			int temp=arr[j];
			arr[j]=arr[j+1];
			arr[j+1]=temp;
		    }
		}
	}
}

void display(int n)
{
	for(int i=0;i<n;i++)
	{
		cout<<arr[i];
	}
}
int main()
{
	int n;
	cout<<"Enter number of elements in a sorted array";
	cin>>n;
	for(int i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	binary(n);
	display(n);
	
	
	
	
	
	
	return 0;
}
